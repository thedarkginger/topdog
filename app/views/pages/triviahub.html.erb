<script> 


$.getJSON('/trivia_demos.json', function(data) {
var items = [] 
$.each(data, function (item, i) {
items.push('<li id="' + i.order + '">' + i.order + ' - ' + i.question + ' - ' + ' A1: ' + ' - ' + i.answer1 + ' - ' + 'Correct Answer' + ' - ' + i.correcta +  '</li>');
});

$('<ul/>', {
    'class': 'my-new-list',
    html: items.join('')
 	}).appendTo('#example');

  });

	
</script> 

